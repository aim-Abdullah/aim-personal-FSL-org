    <validationRules>
        <fullName>Cannot_Modify_INTERNAL_Contact</fullName>
        <active>true</active>
        <description>This validation rule will not allow editing of internal contacts</description>
        <errorConditionFormula>AND(OR( 
ISCHANGED(FirstName), 
ISCHANGED(LastName)), 
CONTAINS(RecordType.Name, &apos;Internal Contact&apos;), 
NOT(ISNEW()),
NOT(OR(CONTAINS($Profile.Name, &apos;Admin&apos;), $Profile.Name = &apos;APPEAR Integration&apos;))
)</errorConditionFormula>
        <errorMessage>Users cannot modify Internal Contacts in APPEAR as the data is loaded by SAP Interface</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_First_Name_Mandatory</fullName>
        <active>true</active>
        <description>This rule is written to make first name of contact mandatory.</description>
        <errorConditionFormula>ISBLANK( FirstName ) &amp;&amp;  $Profile.Name !=&apos;APPEAR Integration&apos; &amp;&amp;  $Profile.Name != &apos;APPEAR Reporting&apos;</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>First Name is Mandatory</errorMessage>
    </validationRules>
