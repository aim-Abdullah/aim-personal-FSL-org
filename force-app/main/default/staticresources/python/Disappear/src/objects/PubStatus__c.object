    <validationRules>
        <fullName>Actual_Date_is_NOT_in_future</fullName>
        <active>true</active>
        <errorConditionFormula>ActualDate__c &gt; NOW()</errorConditionFormula>
        <errorDisplayField>ActualDate__c</errorDisplayField>
        <errorMessage>Please DO NOT Enter an Actual Date in Future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proposal_Complete_Update</fullName>
        <active>true</active>
        <description>Proposal Complete Actual Date can only be edited by BA or GPPL</description>
        <errorConditionFormula>AND
(NOT(OR( CONTAINS($Profile.Name, &apos;Admin&apos;),$Profile.Name = &apos;APPEAR GPPL&apos;)),
 ISCHANGED( ActualDate__c),
  ISPICKVAL(Status__r.PubRecordType__c,&quot;Execute&quot;),
  ISPICKVAL(Status__r.Stage__c,&quot;Execute&quot;),
 Status__r.Name = &quot;Proposal Complete&quot; 
)</errorConditionFormula>
        <errorDisplayField>ActualDate__c</errorDisplayField>
        <errorMessage>Only a Business Administrator or GPPL can enter an actual date for Proposal Complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TargetDate_is_not_Editable</fullName>
        <active>true</active>
        <description>&quot;Target Date&quot; field not allow user to edit it if there is a value present for the Submitted/Completed status only.  The dates of which the changes should be able to be made are between 1/1 and 2/15</description>
        <errorConditionFormula>AND(
ISCHANGED(TargetDate__c),
NOT(ISBLANK(PRIORVALUE(TargetDate__c))),
NOT(OR(MONTH(TODAY()) == 1,And(MONTH(TODAY())==2,DAY(TODAY())&lt;=15))),
Status__r.Name = &apos;Submitted/Completed&apos;,
NOT(CONTAINS($Profile.Name, &apos;Admin&apos;))
)</errorConditionFormula>
        <errorMessage>Once Target Date on Submitted/Completed status is set, please use Latest Target Submission Date for any updates. The target date will be editable the following year from Jan 1st to Feb 15th</errorMessage>
    </validationRules>
