    <validationRules>
        <fullName>Meeting_must_have_a_name</fullName>
        <active>false</active>
        <description>Meeting must have a Meeting Name or a Legacy Meeting Name</description>
        <errorConditionFormula>AND( 
ISNULL( Legacy_Meeting__c  ) , 
ISNULL( Meeting_Name__c  ) 
)</errorConditionFormula>
        <errorMessage>Meeting must have a Meeting Name or a Legacy Meeting Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_End_Must_Be_Added_to_New_Meetings</fullName>
        <active>false</active>
        <description>This will throw an error if the Start or End fields have no value for a record that is entered by a user.</description>
        <errorConditionFormula>AND
($Profile.Name = &apos;APPEAR Business Admin&apos;,
 OR(
 ISNULL( Starts__c ) ,
 ISNULL( Ends__c )
 ) 
)</errorConditionFormula>
        <errorMessage>A Start and End must be entered for new meetings entered by Business Administrators.</errorMessage>
    </validationRules>
